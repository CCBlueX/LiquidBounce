{
  "schemaVersion": 1,
  "id": "liquidbounce",
  "version": "${version}",
  "name": "LiquidBounce",
  "description": "A free mixin-based injection hacked-client for Minecraft using the Fabric modding toolchain.",
  "authors": [
    "CCBlueX"
  ],
  "contact": {
    "homepage": "https://liquidbounce.net/",
    "sources": "https://github.com/CCBlueX/LiquidBounce/",
    "issues": "https://github.com/CCBlueX/LiquidBounce/issues",
    "email": "support@liquidbounce.net"
  },
  "license": "GNU General Public License v3.0",
  "icon": "assets/liquidbounce/icon_64x64.png",
  "environment": "client",
  "entrypoints": {
    "main": []
  },
  "custom": {
    "modmenu": {
      "links": {
        "liquidbounce.generic.guilded": "https://guilded.gg/CCBlueX",
        "liquidbounce.generic.forum": "https://forums.ccbluex.net/"
      }
    }
  },
  "mixins": [
    "liquidbounce.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${fabric_kotlin_version}"
  },
  "suggests": {
    "fabric": ">=${fabric_version}"
  },
  "accessWidener": "liquidbounce.accesswidener"
}
